<h1>Listing communication_devices</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @communication_devices.each do |communication_device| %>
      <tr>
        <td><%= link_to 'Show', communication_device %></td>
        <td><%= link_to 'Edit', edit_communication_device_path(communication_device) %></td>
        <td><%= link_to 'Destroy', communication_device, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Communication device', new_communication_device_path %>
