<h1>Listing Case Managers</h1>

<table class='table'>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @case_managers.each do |case_manager| %>
      <tr>
        <td><%= case_manager.name %><td>
        <td><%= case_manager.email %></td>
        <td><%= link_to 'Show', case_manager %></td>
        <td><%= link_to 'Edit', edit_case_manager_path(case_manager) %></td>
        <td><%= link_to 'Destroy', case_manager, method: :delete, data: { confirm: 'Are you sure?' } if case_manager != current_user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Case manager', new_case_manager_path %>
